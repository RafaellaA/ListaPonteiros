#include <stdio.h>
int main()
{
    char x[4] = "1234";

    printf("Memoria x1[0] = %d\n",x);
    printf("x+1 = %d\n",x+1);
    printf("x+2 = %d\n",x+2);
    printf("x+3 = %d\n",x+3);

    int y[4] = {1,2,3,4};

    printf("Memoria x1[0] = %d\n",y);
    printf("x+1 = %d\n",y+1);
    printf("x+2 = %d\n",y+2);
    printf("x+3 = %d\n",y+3);

    float z[4] = {1,2,3,4};

    printf("Memoria x1[0] = %d\n",z);
    printf("x+1 = %d\n",z+1);
    printf("x+2 = %d\n",z+2);
    printf("x+3 = %d\n",z+3);

    double k[4] = {1,2,3,4};

    printf("Memoria x1[0] = %d\n",k);
    printf("x+1 = %d\n",k+1);
    printf("x+2 = %d\n",k+2);
    printf("x+3 = %d\n",k+3);



    return 0;
}
